#!/usr/bin/env python3
# This file is placed in the Public Domain.


import os


all = ["__import__", "__main__", "__version__", '"## "', "popen", "pkgutil", "importlib", "print", "sys", "traceback"]
skip = ["env", "bin", "dev", "test", ".git", "README", "grep", "mod"]
targets = ["genocide", "op"]
txt  = "grep -R %s %s"


def main():
    for target in targets:
        for t in all:
            greptxt = txt % (t, target)
            for line in os.popen(greptxt).readlines():
                next = False
                for s in skip:
                    if s in line:
                        next = True
                        break
                if next:
                    continue
                print(line.rstrip())


main()
