"command"


## import


import sys


from genocide.obj import Wd, parse, scan
from genocide.hdl import Handler
from genocide.run import command
from genocide.irc import Config


from genocide import cmds, irc, rss


## define

Config.nick = "genocide"
Config.channel = "#genocide"
Config.realname = "Prosecutor. Court. Reconsider OTP-CR-117/19."
Config.username = "genocide"


Wd.workdir = "/var/lib/genocide/"


scan(cmds)
scan(irc)
scan(rss)


## class

class CLI(Client):

    @staticmethod
    def raw(txt):
        print(txt)


## runtime


def main():
    cfg = parse(" ".join(sys.argv[1:]))
    cli = CLI()
    command(cli, cfg.otxt)


main()
