#!/usr/bin/env python3
# This file is placed in the Public Domain.


"cleanup"


import os


def popen(txt):
    for line in os.popen(txt).readlines():
        print(line.rstrip())


popen("rm -fR build dist MANIFEST *~ genocide.egg-info")
popen("rm -fR genocide/*~ genocide/__pycache__")
popen("rm -fR bin/*~ bin/__pycache__")
popen("rm -fR .test test/__pycache__ test/*~ testdir")
popen("rm -fR docs/*~ docs/__pycache__ docs/cide.*.rst docs/gcide.*.rst docs/genocide.*.rst docs/_static/*~ docs/_templates/*~")
popen("rm -fR html")
