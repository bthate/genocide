#!/usr/bin/env python3
# This file is placed in the Public Domain.


"cleanup"


import os


def popen(txt):
    for line in os.popen(txt).readlines():
        print(line.rstrip())


popen("rm -fR build dist MANIFEST *~")
popen("rm -fR genocide/*~ genocide/__pycache__ genocide.egg-info")
popen("rm -fR bin/*~ bin/__pycache__")
popen("rm -fR dev/*~")
popen("rm -fR .test test/__pycache__ test/*~")
popen("rm -fR testdir")
popen("rm -fR docs/*~ docs/genocide.*.rst docs/_static/*~ docs/_templates/*~")
popen("rm -fR html")
